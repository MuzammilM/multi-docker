{
 "AWSEBDockerrunVersion": 2,
 "containerDefinitions" : [ 
	{
		"name": "client",
		"image": "muzammilmomin/multi-client",
		"hostname": "client",
		"essential": false,
		"memory": 128
	},
	{
		"name": "server",
		"image": "muzammilmomin/multi-server",
		"hostname": "api",
		"eseential": false,
                "memory": 128
	},
	{
		"name": "worker",
		"image": "muzammilmomin/multi-worker",
		"hostname": "worker",
		"essential": true,
                "memory": 128
	},
	{
		"name": "nginx",
		"image": "muzammilmomin/multi-nginx",
		"hostname": "nginx",
		"essential": true,
                "memory": 128
		"portMappings": [
			{
				"hostPort": 80,
				"containerPort": 80
			}
			
		],
		"links": ["client", "server"]
	}

 ]
}
